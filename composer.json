{
    "description":  "Magento Testing Framework",
    "keywords":     ["automation", "magento", "testing"],
    "homepage":     "http://www.magentocommerce.com/",
    "license":      "proprietary",
    "require": {
        "php":                                  ">=5.3.3",
        "phpunit/phpunit":                      ">=3.7.18",
        "phpunit/phpunit-selenium":             ">=1.2.12",
        "claylo/selenium-server-standalone":    ">=2.32.0",
        "mtf":                                  "1.0"
    },
    "autoload": {
        "psr-0": {
            "": ["vendor/mtf/framework", "testsuites", "generated", "lib", "tests/app", "tests/design"]
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "mtf",
                "version": "1.0",
                "source": {
                    "type": "git",
                    "url": "https://github.scm.corp.ebay.com/mtf/mtf.git",
                    "reference": "master"
                }
            }
        }
    ]
}
